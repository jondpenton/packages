{
  "name": "@jondpenton/pivotal-story-branch",
  "description": "Pivotal Tracker Story Branch Generator is a tool to automatically generate a branch name for a new story in Pivotal Tracker.",
  "homepage": "https://github.com/jondpenton/packages/tree/master/packages/pivotal-story-branch",
  "version": "0.0.0-development",
  "main": "./dist/index.js",
  "bin": {
    "psb": "./bin/run"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "build:clean": "rimraf ./dist/",
    "build:app": "tsc -p ./tsconfig.build.json",
    "build": "run-s build:*",
    "start": "node ./dist/index.js",
    "dev": "nodemon -e ts --exec \"pnpm run-s build start\"",
    "release": "semantic-release --extends semantic-release-monorepo",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "lint": "run-p lint:*",
    "lint:commit": "commitlint --from HEAD~1 --to HEAD --verbose",
    "lint:format": "prettier -c 'src/**/*.ts'",
    "lint:format:fix": "prettier --write ."
  },
  "dependencies": {
    "@oclif/command": "~1.8.0",
    "@oclif/config": "~1.17.0",
    "@oclif/plugin-help": "~3.2.0",
    "axios": "0.24.0",
    "ora": "~5.0.0",
    "slugify": "~1.6.3",
    "xstate": "~4.26.1",
    "@oclif/errors": "~1.3.5",
    "type-fest": "~2.6.0"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/preset-env": "~7.16.4",
    "@babel/preset-typescript": "~7.16.0",
    "@commitlint/cli": "~15.0.0",
    "@commitlint/config-conventional": "~15.0.0",
    "@oclif/dev-cli": "~1.26.3",
    "@oclif/test": "~1.2.7",
    "@types/jest": "~27.0.3",
    "@types/node": "~16.11.10",
    "babel-jest": "~27.3.1",
    "chai": "~4.3.4",
    "jest": "~27.3.1",
    "msw": "0.33.3",
    "nodemon": "~2.0.4",
    "npm-run-all": "~4.1.5",
    "oclif": "~1.18.1",
    "prettier": "~2.5.0",
    "pretty-quick": "~3.1.2",
    "semantic-release": "~18.0.1",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.2",
    "rimraf": "~3.0.2",
    "@oclif/parser": "~3.8.5",
    "tslib": "~2.3.0",
    "fancy-test": "~1.4.10",
    "semantic-release-monorepo": "~7.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jondpenton/packages.git"
  },
  "oclif": {
    "bin": "psb",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist"
  ]
}
